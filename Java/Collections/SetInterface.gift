    System.out.println(hs);
    System.out.println(ts);
    System.out.println(lhs);
</pre>
{
    ~ Random order, [-1,1,2,3], [1,3,2,-1]
    ~ [-1,1,2,3], Random order, [1,3,2,-1]
    ~ [-1,1,2,3], Random order, [-1,1,2,3]
    = Random order, [-1,1,2,3], [-1,1,2,3]
}

